package dsf;

import java.util.Scanner;

public class GG {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = 0;
		int c = 0 ; 
		for(int i = 1; 1<a; i++) {
			
			if(i%2==0) {
				a= +i;
			}else if(i%2==1) {
				b= + i;
			}
		}
		System.out.println("짝수"+a);
		System.out.println("홀수"+b);
	}

}
