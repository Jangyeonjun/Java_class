
 
package rjsdn;

import java.util.Scanner;

public class Rjadn {
	public static void main(String[] args) {
		//구구단	
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("몇단 보고싶어");
		
		int a = sc.nextInt();
		int c = 1;
		while(c<=9){
							
		System.out.println(a+" * "+c+" = "+(a*c));
		c++;
		}	
		
		
		
	}
	
	
		
	
}

