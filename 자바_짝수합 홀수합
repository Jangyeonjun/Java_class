package lhkl;

public class Ssd {

	public static void main(String[] args) {
		
		int EvenHap = 0;
		int OddHap = 0;
		
		
		
		
		for(int i =1; i<=999; i++) {
			if(i%2 ==0) {
				EvenHap+=i;
			}else {
				OddHap+= i;
			}
		}
		System.out.println("짝수합:"+EvenHap);
		System.out.println("홀수합:"+OddHap);
	}

}
